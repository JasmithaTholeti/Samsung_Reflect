{
  "name": "samsung-reflect-ai",
  "version": "1.0.0",
  "description": "Samsung Reflect AI-powered image search with YOLO, Places365, and CLIP integration",
  "main": "index.js",
  "scripts": {
    "setup": "node scripts/setup.js",
    "dev": "docker-compose up",
    "dev:build": "docker-compose up --build",
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
    "restart": "docker-compose restart",
    "logs": "docker-compose logs -f",
    "test": "bash scripts/test-api.sh",
    "clean": "docker-compose down -v --remove-orphans && docker system prune -f",
    "models:download": "bash scripts/download-models.sh",
    "backup": "bash scripts/backup.sh",
    "restore": "bash scripts/restore.sh"
  },
  "keywords": [
    "ai",
    "computer-vision",
    "yolo",
    "places365",
    "clip",
    "image-search",
    "mern",
    "react",
    "nodejs",
    "mongodb",
    "vector-database"
  ],
  "author": "Samsung Reflect Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/samsung/reflect-ai"
  },
  "engines": {
    "node": ">=18.0.0",
    "docker": ">=20.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  }
}
